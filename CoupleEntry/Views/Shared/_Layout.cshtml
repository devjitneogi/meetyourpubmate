<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="google-signin-client_id" content="254132111059-vqf8bgnklrfrdq6b46pkbvjqaua2mo8l.apps.googleusercontent.com">
    <title>@ViewBag.Title - Find your partner!</title>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="https://apis.google.com/js/api.js"></script>
    <script src="https://apis.google.com/js/client:platform.js?onload=start" async defer></script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAme7silNihhy4-UJNE76I2LUdf2PxkEZY&callback=initMap">
    </script>
    <script src="~/Scripts/script.js"></script>
    @Styles.Render("~/Content/css")

</head>
<body>

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div id="mask" class="mask" style="top: 0px; height: 100%; width: 100%;">
        <div class="loading"></div>
    </div>
    <div id="mainDiv" class="container-fluid body-content">
        @if (IsSectionDefined("Login"))
        {
            @RenderSection("Login");
        }
        @if (IsSectionDefined("Integration"))
        {
            @RenderSection("Integration");
        }  
        @if (IsSectionDefined("AddUser"))
        {
            @RenderSection("AddUser", true);
        }
        @if (IsSectionDefined("Scripts"))
        {
            @RenderSection("Scripts", true);
        }
    </div>


    @Scripts.Render("~/bundles/bootstrap")
  @*  @RenderSection("scripts", required: false)*@

</body>
</html>
